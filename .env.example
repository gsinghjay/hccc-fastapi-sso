# Application Settings
PROJECT_NAME=FastAPI User Management
API_PREFIX=/api
API_V1_STR=v1
DEBUG=false
HOST=0.0.0.0
PORT=8000
RATE_LIMIT_PER_MINUTE=60

# Database Settings
POSTGRES_USER=postgres
POSTGRES_PASSWORD=your-secure-password
POSTGRES_DB=user_management
POSTGRES_PORT=5432

# CORS Settings
ALLOWED_ORIGINS=https://example.com,https://www.example.com

# Domain Settings
DOMAIN=api.example.com

# SSL/TLS Settings
ACME_EMAIL=admin@example.com

# Traefik Dashboard Authentication
TRAEFIK_DASHBOARD_USER=admin
# Generate using: htpasswd -nb admin secure-password
TRAEFIK_DASHBOARD_PASSWORD_HASH=your-hashed-password

# Security Settings
JWT_SECRET_KEY=your-secret-key
JWT_ALGORITHM=HS256
ACCESS_TOKEN_EXPIRE_MINUTES=30
REFRESH_TOKEN_EXPIRE_DAYS=7

# Redis Settings (if needed)
REDIS_HOST=redis
REDIS_PORT=6379
REDIS_PASSWORD=your-redis-password

# Monitoring
PROMETHEUS_METRICS=true
METRICS_PORT=8082

# Development Settings (only for development environment)
DEV_CORS_ORIGINS=http://localhost:3000,http://localhost:8000
DEV_DOMAIN=localhost 